<?php
// src/Ticket/VentasBundle/Form/LoginForm.php
namespace Ticket\VentasBundle\Form;


class LoginForm
{
	protected $user;
	protected $pass;
	public function getUser()
	{
		return $this->user;
	}
	public function setUser($user)
	{
		$this->user = $user;
	}
	public function getPass()
	{
		return $this->pass;
	}
	public function setPass($pass = null)
	{
		$this->pass = $pass;
	}
}
